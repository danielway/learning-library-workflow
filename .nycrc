{
  "cache": false,
  "check-coverage": true,
  "extension": [ ".ts" ],
  "include": [ "src/**/*.ts" ],
  "exclude": [
    "node_modules/**",
    "**/*.d.ts",
    "**/*.test.ts"
  ],
  "sourceMap": true,
  "reporter": [ "text-summary" ],
  "all": true,
  "instrument": true,
  "branches": 90,
  "lines": 90,
  "functions": 90,
  "statements": 90,
  "watermarks": {
    "lines": [ 80, 90 ]
  }
}
